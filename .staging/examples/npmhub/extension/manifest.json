{
  "name": "npmhub",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Explore npm dependencies on GitHub and GitLab repos",
  "author": "Zeke Sikelianos, Federico Brigante",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "npm-hub@sikelianos.com",
      "strict_min_version": "52.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://registry.npmjs.org/"
  ],
  "optional_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "content_scripts":[
    {
      "matches": [
        "https://github.com/*",
        "https://gitlab.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}